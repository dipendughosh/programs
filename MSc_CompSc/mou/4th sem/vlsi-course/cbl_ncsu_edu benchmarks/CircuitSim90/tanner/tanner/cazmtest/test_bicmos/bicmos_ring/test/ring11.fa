*********************************************************
**                                                     **
** BICMOS INVERTER                                     **
**                                                     **
*********************************************************

.subckt inv 1 4 5 0

M1 2 1 5 5 penh l=1U w=33.3U
*M2 2 1 0 0 nenh l=1U w=20U
M2 2 1 0 0 nenh l=1U w=5U
M3 4 1 3 0 nenh l=1U w=20U
*M4 3 2 0 0 nenh l=1U w=20U
M4 3 2 0 0 nenh l=1U w=5U
Q1 5 2 4 0 NPNBJT
Q2 4 3 0 0 NPNBJT

CL 4 0 1.0E-12

.ends inv

******************************************************
 
X01 1  2  25 0 inv
