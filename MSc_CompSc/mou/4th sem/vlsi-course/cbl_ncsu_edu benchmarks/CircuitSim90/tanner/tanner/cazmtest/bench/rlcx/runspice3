# /bin/csh -f

# Script to run SPICE3 on a DS3100

#set DIR=$1
set SPICE3=spice3
set files=(grid.sp grid2.sp gridtran.sp hwang.sp lo.sp) 
set files=( lo.sp)

foreach i ($files)
   set bi=$i:r
   echo "$bi"
   echo "$bi" >>&! $bi.stime
   (/bin/time $SPICE3 -b $bi".sp" > $bi".sout") >>&! $bi.stime
end
