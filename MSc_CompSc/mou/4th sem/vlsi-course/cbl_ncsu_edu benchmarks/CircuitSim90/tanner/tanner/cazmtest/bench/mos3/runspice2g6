# /bin/csh -f

# Script to run SPICE2g6 on a DS3100

#set DIR=$1
set SPICE2g6=spice2g6
#set files=( arom.sp b330.sp counter.sp gm1.sp gm17.sp gm19.sp gm2.sp gm3.sp jge.sp mike2.sp rich3.sp todd3.sp)
set files=( gm3.sp )

foreach i ($files)
   set bi=$i:r
   echo "$bi"
   echo "$bi" >>&! $bi.s2time
   (/bin/time $SPICE2g6 < $bi".sp" > $bi".s2out") >& $bi.s2time
end
