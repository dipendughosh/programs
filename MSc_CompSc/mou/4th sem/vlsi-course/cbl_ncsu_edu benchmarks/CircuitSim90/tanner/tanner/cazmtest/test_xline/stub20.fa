/* This model uses a 1.25um CMOS input as a load */

/* This subcircuit models a 1.25um CMOS input for load modeling */
.subckt cmosinput in out vdd vss
lbond   in  inpad  3.8n  /* Flip-Chip bump inductance */
msource out inpad vdd vdd penh l=1.25u w=3u
msink   out inpad vss vss nenh l=1.25u w=2u
.ends cmosinput

/* The first case shows a properly terminate line for reference. */
tline1 vs1	GND vload1	GND	z0=50 td=0.45ns
rload1 vload1	GND			50

/* This second case has no termination other than the load */
tline2 vs2	GND vload2	GND	z0=50 td=0.45ns
xload2 vload2	po2 vdd  	GND	cmosinput

/* The third case adds the test pad and interconnect unterminated */
tline3 vs3	GND vsum3	GND	z0=50 td=0.25ns
tx31   vsum3	GND vload3	GND	z0=100 td=0.2ns
xload3 vload3	po3 vdd 	GND	cmosinput
tx32   vsum3	GND vtest3	GND	z0=100 td=0.2ns
rtest3 vtest3   GND                     1MEG

/* And, finally, a fully compensated test pad structure */
tline4 vs4	GND vsum4	GND	z0=50 td=0.25ns
tx41   vsum4	GND vload4	GND	z0=100 td=0.2ns
xload4 vload4	po4 vdd         GND	cmosinput
tx42   vsum4	GND vtest4	GND	z0=100 td=0.2ns
rtest4 vtest4	GND			455


