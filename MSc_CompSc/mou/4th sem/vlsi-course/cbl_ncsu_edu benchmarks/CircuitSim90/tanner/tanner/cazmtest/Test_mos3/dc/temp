
nfet_1.N.s2:	$(MODEL) nfet_1.fa nfet.env
		cat $(MODEL) nfet_1.fa nfet.env | expand > spice.in 
		echo ".end" >> spice.in
		spice2g6 < spice.in > spice.out
		awk -f fixgraph.s2.a spice.out > nfet_1.N.s2
		cat nfet_1.N.s2 | \
		 graph -b -g 2 -l nfet_1.N.s2 -x 0 5.5 0.5 -y 0 400 50 -h 0.6035 -w 0.80145 | \
		 lpr -g -Plw2

