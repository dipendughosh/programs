		function ProjectsAJAX()
		{					
			xmlhttp=GetXmlHttpObject();
				if (xmlhttp==null)
				{
					alert ("Browser does not support HTTP Request");
					return;
				}
			connect_server();
		}
		
		function connect_server()
		{
			var PR_id,PR_headname,PR_topic,PR_type,PR_fundedby,PR_status;
						
			document.getElementById('insert_response').innerHTML = "Wait...";
		
			PR_id=document.getElementById('PP_id').value;
			PD_headname=document.getElementById('PD_headname').value;
			PD_topic=document.getElementById('PD_topic').value;
			PD_type=document.getElementById('PD_type').value;
			PD_fundedby=document.getElementById('PD_fundedby').value;
			PD_status=document.getElementById('PD_status').value;
			
			alert(PD_name);
			
			var url="insert_into_personal_details.php";					
				
				url=url+"?PD_name="+PD_name+"&PD_dob="+PD_dob+"&PD_male="+PD_male+"&PD_address="+PD_address+"&PD_pin="+PD_pin+"&PD_landline="+PD_landline+"&PD_mobile="+PD_mobile+"&PD_email="+PD_email+"&PD_doj="+PD_doj+"&PD_quali"+PD_quali+"&PD_type"+PD_type+"&PD_post"+PD_post+"&PD_special"+PD_special;				
				url=url+"&sid="+Math.random();	alert(url);
							
				xmlhttp.open('get',url,true);
				xmlhttp.onreadystatechange=stateChanged;
				xmlhttp.send(null);
		}
		
		function stateChanged()
		{
			if (xmlhttp.readyState==4)
			{
				document.getElementById("insert_response").innerHTML=xmlhttp.responseText;
			}
		}

		function GetXmlHttpObject()
		{
			if (window.XMLHttpRequest)
			{
				// code for IE7+, Firefox, Chrome, Opera, Safari
				return new XMLHttpRequest();
			}
			if (window.ActiveXObject)
			{
				// code for IE6, IE5
				return new ActiveXObject("Microsoft.XMLHTTP");
			}
		return null;
		}
