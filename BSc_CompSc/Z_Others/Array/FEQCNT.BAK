#include<stdio.h>
#include<conio.h>

void main()
{       int arr[100],n,i,j,x=1;
	void replace(int [],int *);
	clrscr();
	printf("Enter number of elements of the array ");
	scanf("%d",&n);
	printf("Enter elements of array\n");
	for(i=0;i<n;i++)
		scanf("%d",&arr[i]);
	for(i=0;i<n;i++)
	{	for(j=i+1;j<n;j++)
		{	if(arr[i]==arr[j])
			{	x++;
				arr[j]=999;
			}
		}
		printf("\nNumber of %d's = %d",arr[i],x);
		arr[i]=999;
		x=1;
		i=0;
		replace(arr,&n);
	}
	getch();
}

void replace(int arr[100],int *n)
{	int i,j;
	for(i=0;i<*n;i++)
	{       if(arr[i]==999)
		{       for(j=i;j<*n;j++)
			{	arr[j]=arr[j+1];
			}
			*n--;
		}
	}

}