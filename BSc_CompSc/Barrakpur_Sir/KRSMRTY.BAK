	scanf("%d",&n1);
	printf("Enter Upper Limit-");
	scanf("%d",&n2);
	for(i=n1;i<n2;i++)
	{       s=0;
		n=i;
		while(n!=0)
		{	a=n%10;
			n=n/10;
			f=factorial(a);
			s=s+f;
		}
		if(i==s)
			printf("%d\t");
	}
	getch();
}

