//krishnamorty Number
#include<stdio.h>
#include<conio.h>

int factorial(int x)
{       int i,f=1;
	for(i=1;i<=x;i++)
		f=f*i;
	return(f);
}

void main()
{	int i,n1,n2,n,s,f,a;
	clrscr();
