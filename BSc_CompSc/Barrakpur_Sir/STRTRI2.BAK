#include<stdio.h>
#include<conio.h>


void main()
{       int i,j,n;
	clrscr();
	printf("\nEnter number of lines :-");
	scanf("%d",&n);
	for(i=1;i<n;i++)
	{	for(j=0;j<=n-i;j++)
			printf("   ");
		for(j=0;j<i;j++)
			printf("*  ");
		printf("\n");
	}
	getch();
}

