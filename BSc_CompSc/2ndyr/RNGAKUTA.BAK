//Runga Kutta Method
#include<stdio.h>
#include<conio.h>

float f(float a,float b)
{	return(.5*(1+a)*b*b);
}

void main()
{       float x[20],y[20],h,k1,k2,k3,k4,k,xn,i;
	int j;
	clrscr();
	printf("\nEnter X0-");
	scanf("%f",&x[0]);
	printf("\nEnter Y0-");
	scanf("%f",&y[0]);
	printf("\nEnter h-");
	scanf("%f",&h);
	printf("\nEnter xn-");
	scanf("%f",&xn);
	printf("\tX\t\tY");
	for(i=0.0,j=0;i<=xn;i=i+h,j++)
	{       k1=h*f(x[j],y[j]);
		k2=h*f(x[j]+h/2,y[j]+k1/2);
		k3=h*f(x[j]+h/2,y[j]+k2/2);
		k4=h*f(x[j]+h,y[j]+k3);
		k=(k1+2*k2+2*k3+k4)/6;
		y[j+1]=y[j]+k;
		x[j+1]=x[j]+h;
		printf("\n\t%f\t\t%f",x[j],y[j]);
	}
	getch();
}

