//insertion sort
#include<stdio.h>
#include<conio.h>

void main()
{	int a[20],i,j,item,n;
	clrscr();
	printf("Enter number of elements:-");
	scanf("%d",&n);
	printf("Enter elements:-\n");
	for(i=0;i<n;i++)
		scanf("%d",&a[i]);
	for(i=0;i<n;i++)
	{	item=a[i];
		for(j=i;j>0;j--)
		{	if(item<a[j-1])
				a[j]=a[j-1];
			else
				break;
		}
		a[j]=item;
	}
	for(i=0;i<n;i++)
		printf("%d\t",a[i]);
	getch();
}

