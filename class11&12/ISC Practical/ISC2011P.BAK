#include<stdio.h>

class lucky
{	int arr[50],n,i,j;
	public:
	lucky()
	{	for(i=0;i<50;i++)
			arr[i]=0;
	}
	lucky(int x)
	{	n=x;
		for(i=0;i<n;i++)
			arr[i]=i+1;
	}
	void showarr(int l)
	{	for(i=0;i<l;i++)
			cout<<arr[i]<<"\t";
	}
	void findluckyno()
	{       int skip=2,l=50,k,i;
		do
		{	k=0;
			for(i=0;i<l;i++)
				arr[k++]=arr[i];
			l=k;
			skip++;
		}while(skip<=l);
		showarr(l);
	}
};
