int fibo()
{	int i,x=0,y=1,z;
	for(i=2;i<=10;i++)
	{	z=x+y;
		x=y;
		y=z;
		return z;
	}
}
int fact(int x)
{	int i,f=1;
	for(i=x;i>=1;i--)
		f=f*i;
	return f;
}









































































































































































