class vector
{	private:
	int arr[20],num,max1,i,j;
	public:
	vector()
	{
	}
	void getdata();
	void modify();
	void max();
	void display();
	~vector()
	{
	}
};

void vector::getdata()
{       cout<<"\nEnter elements of vector-->>\n";
	for(i=0;i<10;i++)
	{	cin>>arr[i];
	}
}

void vector::modify()
{       char y;
	for(i=0;i<9;i++)
	{	for(j=i+1;j<10;j++)
		{	if(arr[i]==arr[j])
			{	cout<<arr[j]<<" is a duplicate ";
				cout<<"\nDo you want to replace it"<<(char)63;
				cin>>y;
				if(y=='y' || y=='Y')
				{	cout<<"\nEnter the replacing element-->";
					cin>>num;
					arr[j]=num;
				}
			}
		}
	}
}

void vector::max()
{	max1=arr[0];
	for(i=1;i<10;i++)
		if(max1<=arr[i])
			max1=arr[i];
	cout<<"\nMaximum number is-->"<<max1<<"\n";
}

void vector::display()
{	for(i=0;i<10;i++)
		cout<<arr[i]<<"\t";
}
